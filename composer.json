{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "ext-apcu": "*",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-redis": "*",
    "ext-sodium": "*",
    "ext-swoole": "*",
    "aws/aws-sdk-php": "^3.294",
    "doctrine/dbal": "^3.7",
    "filament/filament": "^3.1",
    "guzzlehttp/guzzle": "^7.8",
    "http-interop/http-factory-guzzle": "^1.2",
    "laravel/framework": "^v10.37",
    "laravel/horizon": "^v5.21",
    "laravel/octane": "^2.1",
    "laravel/prompts": "^0.1.14",
    "laravel/sanctum": "^3.2",
    "laravel/scout": "^10.2",
    "laravel/telescope": "^4.16",
    "laravel/tinker": "^2.8",
    "livewire/livewire": "^3.1",
    "meilisearch/meilisearch-php": "^v1.5.0",
    "pusher/pusher-php-server": "^7.2",
    "spatie/laravel-permission": "^6.3",
    "spatie/laravel-sitemap": "^7.0",
    "symfony/cache": "^v7.0",
    "tpetry/laravel-postgresql-enhanced": "^0.33"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.9",
    "barryvdh/laravel-ide-helper": "*",
    "brianium/paratest": "^7.2",
    "fakerphp/faker": "^1.9.1",
    "laravel/envoy": "^2.9",
    "laravel/pint": "^1.11",
    "laravel/sail": "^1.26",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^7.0",
    "pestphp/pest": "^2.28",
    "pestphp/pest-plugin-faker": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.2",
    "pestphp/pest-plugin-livewire": "^2.1",
    "pestphp/pest-plugin-watch": "^2.0",
    "phpunit/phpunit": "^10.1",
    "spatie/laravel-ignition": "^2.0",
    "swoole/ide-helper": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "@php artisan test --parallel --recreate-databases --coverage",
    "test:coverage:ci": "@php artisan test --parallel --recreate-databases --coverage --coverage-clover=coverage.xml",
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan ide-helper:generate -n",
      "@php artisan ide-helper:meta -n",
      "@php artisan ide-helper:models -n",
      "@php artisan ide-helper:eloquent -n",
      "@php artisan filament:upgrade"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    },
    "preload": {
      "exclude": {
        "laragear/meta": [
          "src/Cache/*",
          "resources/views"
        ],
        "charlesdp/builder": true
      }
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
